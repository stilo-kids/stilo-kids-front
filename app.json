{
  "expo": {
    "name": "Stilo Kids",
    "slug": "stilo-kids-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/app/assets/images/leao.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/app/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#6DC4C5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/app/assets/images/leao.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
